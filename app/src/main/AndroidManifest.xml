<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.edu.uco.insales">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Insales">
        <activity android:name=".ActualizarProductoActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".NuevoProductoActivity" android:screenOrientation="portrait"/>
        <activity android:name=".DetalleProductoActivity" android:screenOrientation="portrait" />
        <activity android:name=".ActualizarProveedorActivity" android:screenOrientation="portrait" />
        <activity android:name=".DetalleProveedorActivity" android:screenOrientation="portrait"/>
        <activity android:name=".NuevoProveedorActivity" android:screenOrientation="portrait"/>
        <activity android:name=".VentasActivity" android:screenOrientation="portrait"/>
        <activity android:name=".ProductoActivity" android:screenOrientation="portrait"/>
        <activity android:name=".ProveedorActivity" android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>